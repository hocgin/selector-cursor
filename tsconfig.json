{
  "compilerOptions": {
    "outDir": "dist",
    "strict": false,
    "allowJs": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@hocgin/selector-cursor": [
        "src"
      ],
      "@hocgin/selector-cursor/*": [
        "src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*"
  ]
}
